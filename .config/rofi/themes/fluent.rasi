/* Fluent Theme */
* {
  bg:         #0104084A; //window
  main-bg:    #02080F6A; //main
  main-br:    #00BBEE88; //windowBorder
  main-fg:    #DEDEDE;   //main
  input-bg:   #0000000F; //input
  input-br:   #242424;   //inputBorder
  select-fg:  #33BBFF;   //selected
  select-bg:  #00000088; //selected

  black   : #000000;
  red     : #F7768E;
  green   : #9ECE6A;
  yellow  : #E0AF68;
  blue    : #7AA2F7;
  magenta : #9AAECC;
  grey    : #BCBDBF;
  cyan    : #4ABAEF;

  accent: @cyan;
  urgent: @yellow;
}
